<h1>Listing user_books</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Book</th>
    <th>Twitter</th>
    <th>Rental flag</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_books.each do |user_book| %>
  <tr>
    <td><%= user_book.id %></td>
    <td><%= user_book.book_id %></td>
    <td><%= user_book.twitter_id %></td>
    <td><%= user_book.rental_flag %></td>
    <td><%= link_to 'Show', user_book %></td>
    <td><%= link_to 'Edit', edit_user_book_path(user_book) %></td>
    <td><%= link_to 'Destroy', user_book, :confirm => 'Are you sure?',
    :method => :delete %></td>
 </tr>
<% end %>
</table>

